# .env.example

# Ethereum network provider URL
WEB3_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Contract address for the cryptocurrency (e.g., USDT)
CRYPTO_CONTRACT_ADDRESS=0xYourCryptoContractAddress

# Chain ID for the Ethereum network (Sepolia Testnet)
CHAIN_ID=11155111

# Gas limit for transactions
GAS=2000000

# Wallet addresses for agents
AGENT1_WALLET_ADDRESS=0xYourAgent1WalletAddress
AGENT1_PRIVATE_KEY=YourAgent1PrivateKey

AGENT2_WALLET_ADDRESS=0xYourAgent2WalletAddress
AGENT2_PRIVATE_KEY=YourAgent2PrivateKey

# OpenAI API Key
OPENAI_API_KEY=YourOpenAIAPIKey